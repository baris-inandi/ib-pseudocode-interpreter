STOCK = [1,4,68,2,4,5,9,23,5,99]
COUNT = 0
TOTAL = 0
loop N from 0 to len(STOCK)-1
    if STOCK[N] > 0 then
        COUNT = COUNT + 1
        TOTAL = TOTAL + STOCK[N]
    end if
end loop
if COUNT != 0 then
    AVERAGE = TOTAL / COUNT
    output "Average = " , AVERAGE
else
    output "There are no non-zero values"
end if